<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <center>
      <BorderPane BorderPane.alignment="CENTER">
         <center>
            <GridPane fx:id="centerGrid" alignment="CENTER">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <Button fx:id="chooseFileButton" mnemonicParsing="false" onAction="#chooseFile" text="chooseFile" />
                  <Button fx:id="seekButton" mnemonicParsing="false" onAction="#seek" text="seek +5" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                  <ToggleButton fx:id="togglePlayPause" mnemonicParsing="false" onAction="#playpause" text="Play" GridPane.rowIndex="2" />
                  <Button fx:id="loadBDButton" mnemonicParsing="false" onAction="#loadDB" text="Load DB" GridPane.columnIndex="2" />
               </children>
            </GridPane>
         </center>
         <bottom>
            <GridPane fx:id="bottomGrid" BorderPane.alignment="CENTER">
              <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="115.0" minWidth="10.0" prefWidth="48.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="434.0" minWidth="10.0" prefWidth="423.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="138.0" minWidth="10.0" prefWidth="61.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="149.0" minWidth="10.0" prefWidth="39.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="104.0" minWidth="10.0" prefWidth="46.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="100.0" minWidth="10.0" prefWidth="79.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Slider fx:id="volumeSlider" onMouseDragged="#setVolume" prefHeight="33.0" prefWidth="61.0" value="100.0" GridPane.columnIndex="5" />
                  <Label text="vol:" GridPane.columnIndex="3" />
                  <Label fx:id="minDurationLabel" text="Min dur" />
                  <Label fx:id="maxDurationLabel" text="Max dur" GridPane.columnIndex="2" />
                  <Slider fx:id="durationSlider" onMouseClicked="#dragDuration" onMouseDragged="#dragDuration" prefHeight="14.0" prefWidth="370.0" GridPane.columnIndex="1" />
                  <Label fx:id="volumeLabel" prefHeight="17.0" prefWidth="38.0" text="100%" GridPane.columnIndex="4" />
               </children>
               <BorderPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </BorderPane.margin>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </GridPane>
         </bottom>
         <top>
            <Label fx:id="musicNameLabel" text="musicname" BorderPane.alignment="CENTER" />
         </top>
      </BorderPane>
   </center>
   <padding>
      <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
   </padding>
   <left>
      <FlowPane fx:id="musicListFlowPane" prefWidth="173.0" BorderPane.alignment="CENTER" />
   </left>
</BorderPane>
